###############################################################
####### Configuracion Nginx para _CONTAINER_STORE_ ##########
###############################################################

server {

  listen 80;
  listen [::]:80;
  server_name _PROYECT_NAME_COMPLETE_._EXT_;

  index index.php index.html;
  error_log  /var/log/nginx/error.log;
  access_log /var/log/nginx/access.log;

  root /var/www/html/www__CONTAINER_STORE_;

location ~ \.php$ {
    fastcgi_pass _PHPFPM_:9000;
    include /etc/nginx/includes/site.conf;
  }

  include /etc/nginx/includes/generalsite.conf;
}